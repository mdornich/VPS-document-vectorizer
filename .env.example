# Google Drive Configuration
GOOGLE_DRIVE_FOLDER_ID=your_folder_id_here
GOOGLE_CREDENTIALS_PATH=config/credentials.json
GOOGLE_TOKEN_PATH=config/token.json

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_service_role_secret_key

# OpenAI Configuration
OPENAI_API_KEY=sk-your-openai-api-key

# RATE LIMITING - CRITICAL FOR COST CONTROL
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_MINUTE=10
MAX_REQUESTS_PER_HOUR=100
MAX_REQUESTS_PER_DAY=1000
MAX_DAILY_COST_USD=10.00
COST_PER_EMBEDDING=0.0001
COST_PER_COMPLETION=0.01

# Processing Configuration
MONITOR_INTERVAL=60
BATCH_SIZE=10
CHUNK_SIZE=400
CHUNK_OVERLAP=100
MAX_RETRIES=3
TEMP_DOWNLOAD_DIR=/tmp/document_downloads

# Table Names
SUPABASE_DOCUMENTS_TABLE=documents
SUPABASE_METADATA_TABLE=document_metadata
SUPABASE_ROWS_TABLE=document_rows
